#include <stdio.h>
int main (int argc,char **argv)
{
	int i=0;
	if(argc-1 != 2)
	{
		printf("Il faut 2 arguments.\n");
		return 1;
	}
	if(atoi(argv[2]) <= 0)
	{
		printf("Il faut que le deuxiÃ¨me arguments soit un entier strictement positif\n");
		return 1;
	}
	for (i=0;i<atoi(argv[2]);i++)
	{
		printf("%s\n",argv[1]);
	}
return 0;
}
